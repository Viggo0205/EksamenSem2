@page
@model TilføjMedarbejderModel
@{
}
<form method="post">
    <div class="form-group">
        <label asp-for="Medarbejder.Navn" class="control-label"></label>
        <input asp-for="Medarbejder.Navn" class="form-control" />
        <sPan asp-validation-for="Medarbejder.Navn" class="text-danger"></sPan>
    </div>
    <div class="form-group">
        <label asp-for="Medarbejder.Email" class="control-label"></label>
        <input asp-for="Medarbejder.Email" class="form-control" />
        <sPan asp-validation-for="Medarbejder.Email" class="text-danger"></sPan>
    </div>
    <div class="form-group">
        <label asp-for="Medarbejder.TlfNr" class="control-label"></label>
        <input asp-for="Medarbejder.TlfNr" class="form-control" />
        <sPan asp-validation-for="Medarbejder.TlfNr" class="text-danger"></sPan>
    </div>
    <div class="form-group">
        <label asp-for="Medarbejder.Password" class="control-label"></label>
        <input asp-for="Medarbejder.Password" class="form-control" />
        <sPan asp-validation-for="Medarbejder.Password" class="text-danger"></sPan>
    </div>

    <div class="form-group">
        <label asp-for="RolleId">Rolle</label>
        <select asp-for="RolleId" asp-items="@Model.SelectedRolle" class="form-control"></select>
        <span asp-validation-for="RolleId" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="Medarbejder.Timeløn" class="control-label"></label>
        <input asp-for="Medarbejder.Timeløn" class="form-control" />
        <sPan asp-validation-for="Medarbejder.Timeløn" class="text-danger"></sPan>
    </div>
    <div class="form-group">
        <label class="control-label">Kompetencer</label>
        <select asp-for="@Model.SelectedKompetence" asp-items="@Model.Kompetencer" class="form-control"></select>
    </div>
    <div class="form-group">
        <input type="submit" value="Tilføj" class="btn btn-primary" />
    </div>

</form>